; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "Board",
"language": "GDScript",
"path": "res://Board.gd"
}, {
"base": "Node2D",
"class": "Game",
"language": "GDScript",
"path": "res://Game.gd"
}, {
"base": "Node2D",
"class": "Piece",
"language": "GDScript",
"path": "res://Piece.gd"
}, {
"base": "Node2D",
"class": "RecipeOverlay",
"language": "GDScript",
"path": "res://RecipeOverlay.gd"
} ]
_global_script_class_icons={
"Board": "",
"Game": "",
"Piece": "",
"RecipeOverlay": ""
}

[application]

config/name="HAKOSAN"
run/main_scene="res://Game.tscn"
config/icon="res://sprites/hakosan-logo.png"

[autoload]

SettingsManager="*res://autoload/SettingsManager.tscn"
SaveManager="*res://autoload/SaveManager.tscn"
AudioManager="*res://autoload/AudioManager.tscn"
Audio="*res://autoload/Audio.gd"

[display]

window/size/width=600
window/handheld/orientation="portrait"
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.341176, 0.560784, 0.505882, 1 )
environment/default_environment="res://default_env.tres"
